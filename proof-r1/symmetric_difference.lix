# A Δ B = (A - B) ∪ (B - A)

let A set, B set
claim:
    A Δ B = (A - B) ∪ (B - A)
    prove:
        forall x obj:
            x $in (A Δ B) 
            <=> 
            (x $in A and not x $in B) or (x $in B and not x $in A)
            <=> 
            x $in ( (A - B) ∪ (B - A) )
        then:
            A Δ B = (A - B) ∪ (B - A)
